<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="mxc_dropship_innocigs.subscriber.templates" class="MxcDropshipInnocigs\Subscriber\TemplateRegistration">
            <argument>%mxc_dropship_innocigs.plugin_dir%</argument>
            <argument type="service" id="template"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="mxc_dropship_innocigs.subscriber.innocigs_article_subscriber" class="MxcDropshipInnocigs\Subscriber\InnocigsArticleSubscriber">
            <tag name="doctrine.event_subscriber"/>
        </service>
    </services>
</container>
